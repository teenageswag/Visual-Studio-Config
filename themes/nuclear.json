{
  "name": "Nuclear",
  "type": "dark",
  "colors": {
    "editor.background": "#181818",
    "editor.foreground": "#E4E3E8",
    "activityBar.background": "#212121",
    "sideBar.background": "#212121",
    "sideBarSectionHeader.background": "#212121",
    "statusBar.background": "#212121",
    "titleBar.activeBackground": "#212121",
    "editorGroupHeader.tabsBackground": "#212121",
    "tab.activeBackground": "#181818",
    "tab.inactiveBackground": "#212121",
    "tab.border": "#181818",
    "editorCursor.foreground": "#CCB333",
    "editor.selectionBackground": "#94979E33",
    "editor.lineHighlightBackground": "#212121",
    "editorIndentGuide.background": "#212121",
    "editorIndentGuide.activeBackground": "#CCB33340",
    "editorLineNumber.foreground": "#94979E66",
    "editorLineNumber.activeForeground": "#E4E3E8",
    "editorWhitespace.foreground": "#3B4048",
    "editorWidget.background": "#212121",
    "editorWidget.border": "#CCB333",
    "focusBorder": "#CCB333",
    "input.background": "#181818",
    "list.activeSelectionBackground": "#CCB33333",
    "list.hoverBackground": "#212121",
    "scrollbarSlider.background": "#94979E33",
    "scrollbarSlider.hoverBackground": "#94979E66",
    "scrollbarSlider.activeBackground": "#94979E99"
  },
  "tokenColors": [
    {
      "name": "Keywords & Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#CCB333"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#73B82E"
      }
    },
    {
      "name": "Variables & Parameters",
      "scope": [
        "variable",
        "entity.name.variable",
        "parameter",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#E4E3E8"
      }
    },
    {
      "name": "Functions & Types",
      "scope": [
        "entity.name.function",
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#E4E3E8"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#94979E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#E4E3E8"
      }
    },
    {
      "name": "Punctuation & Delimiters",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#94979E"
      }
    },
    {
      "name": "Errors",
      "scope": "invalid",
      "settings": {
        "foreground": "#B82E2E"
      }
    },
    {
      "name": "Object Keys (JSON/JS)",
      "scope": [
        "support.type.property-name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#E4E3E8"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#CCB333"
      }
    },
    {
      "name": "HTML/XML Attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#E4E3E8"
      }
    }
  ]
}
